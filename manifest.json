{
"manifest_version": 3,
"name": "Twitch Auto Redeem",
"description": "Automatically redeems channel points on Twitch.",
"version": "1.0.0",

"icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },

  


"action": {
    "default-popup": "index.html"
    },

    "permissions": ["scripting", "storage"],

"background": {
    "service_worker": "auto-redeem.js"
    }
}
